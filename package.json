  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/emoji-mart": "^3.0.2",
    "@types/faker": "^4.1.3",
    "@types/file-saver": "^2.0.0",
    "@types/google-protobuf": "^3.2.7",
    "@types/googlemaps": "^3.30.19",
    "@types/jest": "^23.3.1",
    "@types/lodash": "^4.14.116",
    "@types/markerclustererplus": "^2.1.33",
    "@types/moment-jalaali": "^0.7.4",
    "@types/node": "14.14.9",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.3",
    "@types/react-avatar-editor": "^10.3.2",
    "@types/react-dom": "^17.0.3",
    "@types/react-dropzone": "^4.2.2",
    "@types/react-router-dom": "^5.1.6",
    "@types/react-textarea-autosize": "^4.3.3",
    "@types/react-virtualized": "^9.21.2",
    "@types/react-virtualized-auto-sizer": "^1.0.0",
    "@types/react-window": "^1.8.1",
    "@types/shortid": "^0.0.29",
    "@types/xregexp": "^3.0.29",
    "electron": "^12.0.6",
    "electron-builder": "^22.10.5",
    "electron-devtools-installer": "^3.1.1",
    "glob": "^7.1.6",
    "grpc": "^1.12.4",
    "node-gyp": "^4.0.0",
    "node-pre-gyp": "^0.13.0",
    "node-sass-chokidar": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "react-scripts": "^4.0.3",
    "ts-protoc-gen": "0.9.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.1.0",
    "type-fest": "^0.20.2",
    "typescript": "^4.2.3",
    "worker-loader": "^3.0.6"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript",
      "react-app"
    ],
    "plugins": [
      "@babel/plugin-proposal-optional-chaining"
    ]
  },
  "build": {
    "appId": "com.river.im",
    "compression": "maximum",
    "productName": "River",
    "directories": {
      "buildResources": "build",
      "output": "dist"
    },
    "mac": {
      "icon": "assets/icon.png",
      "artifactName": "${name}-mac-${version}.${ext}",
      "category": "public.app-category.social-networking",
      "darkModeSupport": true,
      "hardenedRuntime": true,
      "entitlements": "./build/entitlements.mac.plist",
      "extendInfo": {
        "NSCameraUsageDescription": "This app requires camera access to record video.",
        "NSMicrophoneUsageDescription": "This app requires microphone access to record audio."
      },
      "cscLink": "./build/certificates-river-mac.p12",
      "cscKeyPassword": "k1 is not internet",
      "target": [
        "dmg"
      ]
    },
    "win": {
      "target": "nsis",
      "icon": "assets/icon.png",
      "artifactName": "${name}-win-${version}.${ext}"
    },
    "linux": {
      "icon": "assets/icon.png",
      "artifactName": "${name}-linux-${version}.${ext}",
      "target": [
        "deb"
      ],
      "description": "River chat",
      "category": "Network;Messaging"
    },
    "nsis": {
      "deleteAppDataOnUninstall": "true"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "ordered-imports": "off",
      "semi": "warn",
      "no-bitwise": "off",
      "valid-jsdoc": "off",
      "sort-keys": [
        "warn",
        "asc",
        {
          "caseSensitive": true,
          "natural": false
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
